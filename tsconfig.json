{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "UMD",
    "moduleResolution": "node",
    "experimentalDecorators": false,
    "declaration": false,
    "removeComments": true,
    "noEmit": true,
    "allowJs": true,
    "strictNullChecks": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "preserveWatchOutput": true,
    "strict": true,
    "lib": ["es6", "es7", "dom"]

  },
  "exclude": ["dist", "node_modules"]
}